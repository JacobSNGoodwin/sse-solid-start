{
  "name": "sse-solid-start",
  "scripts": {
    "dev": "concurrently -c magenta,blue -n app,db \"pnpm dev:solid\" \"pnpm dev:db\"",
    "dev:solid": "solid-start dev",
    "dev:db": "json-server -w db.json -p 3001 -d 150",
    "build": "solid-start build",
    "start": "concurrently -c magenta,blue -n app,db \"pnpm start:solid\" \"pnpm start:db\"",
    "start:solid": "solid-start start",
    "start:db": "json-server -w db.json -p 3001 -d 150"
  },
  "type": "module",
  "devDependencies": {
    "@types/node": "^18.11.9",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "esbuild": "^0.14.54",
    "json-server": "^0.17.1",
    "postcss": "^8.4.20",
    "solid-start-node": "^0.2.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.4",
    "vite": "^3.1.8"
  },
  "dependencies": {
    "@solidjs/meta": "^0.28.0",
    "@solidjs/router": "^0.6.0",
    "solid-js": "^1.6.2",
    "solid-start": "^0.2.0",
    "undici": "^5.11.0"
  },
  "engines": {
    "node": ">=16.8"
  }
}
